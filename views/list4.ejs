<body>
    <div>
        <%- include("./common/nav")%>
    </div>

    <div class="container">

        <div class="blank-list"></div>

        <div class="row">
            <div class="col">
            </div>
            <div class="col-4" align="center">
                <li class="list-group-item " style="border-radius: 30px;">
                    <h4>
                        <!-- <div class="blank">1</div> -->
                        <span class="text1">
                            <%= posts[0].name %>
                                <%= posts[0].score %>
                        </span>
                    </h4>
            </div>
            <div class="col">
            </div>
        </div>

        <div class="row">
            <div class="col" align="center">
                <li class="list-group-item ">
