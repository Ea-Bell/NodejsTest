                    <h4><span class="text3">
                            <%= posts[2].name %>
                                <%= posts[2].score %>
                        </span>
                    </h4>
                </li>
            </div>
        </div>

        <ul class="list-group ">
                <% for (var i=3; i < posts.length; i++){ %>
                <div class="blank-list"></div>
                <li class="list-group-item " style="border-radius: 15px;">
                    <div align="center">
                        <h4><span class="textRemains">
                                <%= posts[i].name %>
                                    <%= posts[i].score %>
                            </span>
                        </h4>
                    </div>
                </li>
                <% } %>
        </ul>

    </div>
    </div>

















    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"
